  have_epoll_lib="no"
fi

    if test "$HAVE_EPOLL_H $have_epoll_lib" = "yes no"; then
        { echo "$as_me:$LINENO: Checking for epoll_create without -lepoll" >&5
echo $ECHO_N "Checking for epoll_create without -lepoll... $ECHO_C" >&6; }
