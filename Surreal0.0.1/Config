	IRC_CFLAGS="$IRC_CFLAGS -Wall -Wshadow -Wmissing-declarations"
else
	ac_cv_maintainer="no"
fi

if test "$ac_cv_debugmode" = "yes" ; then
	cat >>confdefs.h <<\_ACEOF
