if test "${enable_show_headers+set}" = set; then
  enableval=$enable_show_headers;  ac_cv_show_headers=$enableval
else
   ac_cv_show_headers='yes'
fi


# Check whether --enable-acebot_rejection was given.
if test "${enable_acebot_rejection+set}" = set; then
  enableval=$enable_acebot_rejection;  ac_cv_acebot_rejection=$enableval
else
   ac_cv_acebot_rejection='no'
fi


